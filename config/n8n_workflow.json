      },
      {
        "parameters": {
          "functionCode": "return [{ json: {\n  message: 'Agent event received',\n  event: $json.event,\n  data: $json.data\n} }];"
        },
        "name": "Log",
        "type": "n8n-nodes-base.function",
        "typeVersion": 1,
        "position": [500, 300]
      }
    ],
    "connections": {
      "Webhook": {
        "main": [
          [
            {
              "node": "Log",
              "type": "main",
              "index": 0
            }
          ]
        ]
      }
    }
  }
]
